<div class="share_header">Share with Friends!</div>
<em>Want to see how you stack up against others you know?</em><br/>
<%= form_tag "/send_stack_form" do -%>
<table class="share_form">
  <tr>
	<td class="share_labels">
	  <label for="email">Email:</label><br/>
	  <!-- <label for="from_email">From:</label> -->
	</td>
	<td class="share_fields">
	  <%= text_field "contact", "email", :size => 40 %><br/>
	  <!-- <%= text_field "contact", "from_email", :size => 40, :value => session[:from_email] %> -->
	</td>
  </tr>
</table>
<input type="submit" value="Share with a Friend" class="share_button round" />
<% end -%>

